        border: 1px solid rgba(255, 255, 255, 0.9);
    }
    
    .login input[type=button]:focus {
        outline: none;
    }
    
     ::-webkit-input-placeholder {
        color: rgba(255, 255, 255, 0.6);
    }
    
     ::-moz-input-placeholder {
        color: rgba(255, 255, 255, 0.6);
    }
    
    button {
        display: block;
        width: 80%;
        border: none;
        background-color: greenyellow;
        padding: 14px 28px;
        font-size: 16px;
        cursor: pointer;
        text-align: center;
    }
